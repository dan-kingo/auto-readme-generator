{
  "name": "auto-readme-generator",
  "version": "1.0.0",
  "description": "Automatically generate and update README.md files with AI-powered descriptions, folder structure, and feature extraction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "auto-readme": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "husky install",
    "postinstall": "npm run build && node dist/setup.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "readme",
    "generator",
    "documentation",
    "ai",
    "git-hooks",
    "automation"
  ],
  "author": "Dan-Kingo danieldejen23@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "inquirer": "^8.2.6",
    "mime-types": "^2.1.35",
    "ora": "^5.4.1",
    "semver": "^7.5.4",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dan-kingo/auto-readme-generator.git"
  },
  "bugs": {
    "url": "https://github.com/dan-kingo/auto-readme-generator/issues"
  },
  "homepage": "https://github.com/dan-kingo/auto-readme-generator#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}